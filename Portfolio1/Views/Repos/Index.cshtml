@model IEnumerable<Portfolio.Models.Repos>

@{
    ViewData["Title"] = "PORTFOLIO";
}
<h1 class="page-title">@ViewData["Title"]</h1>

<div class="page-body">
    <div class="row">
        @foreach(var repo in Model)
        {
    	    <div class="col-md-4">
    		    <div class="well">
                    <h4 class="card-title">@repo.name</h4>
    			    <a href="https://github.com/@repo.full_name">go</a>
    	    	</div>
            </div>
        }
    </div>
</div>
